<ul>
<div class="row">
  <div class="large-12 columns">
    <h1 class="tag-line"> Where Jet Setters Talk</h1>
    <ul class="flex-container">
    <% @airlines.each do |airline| %>
      <li class="airline_box">
      <%= link_to airline.name, airline_path(airline) %>
      <img src="<%= airline.image_logo %>">
      Jet Setters say:
      <%=airline.overall_rating%><i class="fas fa-star"></i>
      </li>
    <% end %>
    </ul>

  </div>
</div>
<h5><%= link_to "Add New Airline", new_airline_path %></h5>
</ul>
